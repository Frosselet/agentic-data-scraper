<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns="https://agentic-data-scraper.com/ontology/sow#"
         xml:base="https://agentic-data-scraper.com/ontology/sow"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:swrl="http://www.w3.org/2003/11/swrl#"
         xmlns:swrlb="http://www.w3.org/2003/11/swrlb#">

  <owl:Ontology rdf:about="https://agentic-data-scraper.com/ontology/sow">
    <owl:versionIRI rdf:resource="https://agentic-data-scraper.com/ontology/sow/v1.0"/>
    <rdfs:label>SOW Inference Rules Ontology</rdfs:label>
    <rdfs:comment>OWL ontology defining intelligent inference rules for Semantic Statement of Work processing</rdfs:comment>
  </owl:Ontology>

  <!-- Core Classes -->
  <owl:Class rdf:about="#BusinessRequirement">
    <rdfs:label>Business Requirement</rdfs:label>
    <rdfs:comment>A requirement explicitly stated by business stakeholders</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#InferredOpportunity">
    <rdfs:label>Inferred Opportunity</rdfs:label>
    <rdfs:comment>An analytical opportunity inferred from business requirements</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#SpatialAnalysisOpportunity">
    <rdfs:subClassOf rdf:resource="#InferredOpportunity"/>
    <rdfs:label>Spatial Analysis Opportunity</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="#TemporalAnalysisOpportunity">
    <rdfs:subClassOf rdf:resource="#InferredOpportunity"/>
    <rdfs:label>Temporal Analysis Opportunity</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="#NetworkAnalysisOpportunity">
    <rdfs:subClassOf rdf:resource="#InferredOpportunity"/>
    <rdfs:label>Network Analysis Opportunity</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="#CrossDomainOpportunity">
    <rdfs:subClassOf rdf:resource="#InferredOpportunity"/>
    <rdfs:label>Cross Domain Opportunity</rdfs:label>
  </owl:Class>

  <!-- Domain-Specific Classes -->
  <owl:Class rdf:about="#SupplyChainEntity">
    <rdfs:label>Supply Chain Entity</rdfs:label>
    <rdfs:comment>Any entity mentioned in supply chain context</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#FinancialEntity">
    <rdfs:label>Financial Entity</rdfs:label>
    <rdfs:comment>Any entity mentioned in financial context</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#CommodityEntity">
    <rdfs:label>Commodity Entity</rdfs:label>
    <rdfs:comment>Any entity mentioned in commodities context</rdfs:comment>
  </owl:Class>

  <!-- Properties -->
  <owl:ObjectProperty rdf:about="#mentionsEntity">
    <rdfs:label>mentions entity</rdfs:label>
    <rdfs:domain rdf:resource="#BusinessRequirement"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#infersOpportunity">
    <rdfs:label>infers opportunity</rdfs:label>
    <rdfs:domain rdf:resource="#BusinessRequirement"/>
    <rdfs:range rdf:resource="#InferredOpportunity"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="#confidenceLevel">
    <rdfs:label>confidence level</rdfs:label>
    <rdfs:domain rdf:resource="#InferredOpportunity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#reasoningTrace">
    <rdfs:label>reasoning trace</rdfs:label>
    <rdfs:domain rdf:resource="#InferredOpportunity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <!-- SWRL Inference Rules -->

  <!-- Rule 1: Supplier Performance → Geographic Risk Analysis -->
  <swrl:Imp rdf:about="#SupplierPerformanceRule">
    <swrl:body>
      <swrl:AtomList>
        <rdf:first>
          <swrl:ClassAtom>
            <swrl:classPredicate rdf:resource="#BusinessRequirement"/>
            <swrl:argument1 rdf:resource="#req"/>
          </swrl:ClassAtom>
        </rdf:first>
        <rdf:rest>
          <swrl:AtomList>
            <rdf:first>
              <swrl:DatavaluedPropertyAtom>
                <swrl:propertyPredicate rdf:resource="#containsKeyword"/>
                <swrl:argument1 rdf:resource="#req"/>
                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">supplier</swrl:argument2>
              </swrl:DatavaluedPropertyAtom>
            </rdf:first>
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
          </swrl:AtomList>
        </rdf:rest>
      </swrl:AtomList>
    </swrl:body>
    <swrl:head>
      <swrl:AtomList>
        <rdf:first>
          <swrl:ClassAtom>
            <swrl:classPredicate rdf:resource="#SpatialAnalysisOpportunity"/>
            <swrl:argument1 rdf:resource="#spatialOpp"/>
          </swrl:ClassAtom>
        </rdf:first>
        <rdf:rest>
          <swrl:AtomList>
            <rdf:first>
              <swrl:IndividualPropertyAtom>
                <swrl:propertyPredicate rdf:resource="#infersOpportunity"/>
                <swrl:argument1 rdf:resource="#req"/>
                <swrl:argument2 rdf:resource="#spatialOpp"/>
              </swrl:IndividualPropertyAtom>
            </rdf:first>
            <rdf:rest>
              <swrl:AtomList>
                <rdf:first>
                  <swrl:DatavaluedPropertyAtom>
                    <swrl:propertyPredicate rdf:resource="#confidenceLevel"/>
                    <swrl:argument1 rdf:resource="#spatialOpp"/>
                    <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.85</swrl:argument2>
                  </swrl:DatavaluedPropertyAtom>
                </rdf:first>
                <rdf:rest>
                  <swrl:AtomList>
                    <rdf:first>
                      <swrl:DatavaluedPropertyAtom>
                        <swrl:propertyPredicate rdf:resource="#reasoningTrace"/>
                        <swrl:argument1 rdf:resource="#spatialOpp"/>
                        <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Supplier mentions imply geographic risk analysis needs</swrl:argument2>
                      </swrl:DatavaluedPropertyAtom>
                    </rdf:first>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </swrl:AtomList>
                </rdf:rest>
              </swrl:AtomList>
            </rdf:rest>
          </swrl:AtomList>
        </rdf:rest>
      </swrl:AtomList>
    </swrl:head>
  </swrl:Imp>

  <!-- Rule 2: Cost Tracking → Temporal Trend Analysis -->
  <swrl:Imp rdf:about="#CostTrackingRule">
    <swrl:body>
      <swrl:AtomList>
