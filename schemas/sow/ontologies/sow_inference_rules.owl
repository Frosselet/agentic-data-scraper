                  </swrl:DatavaluedPropertyAtom>
                </rdf:first>
                <rdf:rest>
                  <swrl:AtomList>
                    <rdf:first>
                      <swrl:DatavaluedPropertyAtom>
                        <swrl:propertyPredicate rdf:resource="#reasoningTrace"/>
                        <swrl:argument1 rdf:resource="#networkOpp"/>
                        <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compliance requirements infer regulatory network analysis needs</swrl:argument2>
                      </swrl:DatavaluedPropertyAtom>
                    </rdf:first>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
