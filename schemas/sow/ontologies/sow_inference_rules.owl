            <swrl:argument1 rdf:resource="#req"/>
          </swrl:ClassAtom>
        </rdf:first>
        <rdf:rest>
          <swrl:AtomList>
            <rdf:first>
              <swrl:DatavaluedPropertyAtom>
                <swrl:propertyPredicate rdf:resource="#containsKeyword"/>
                <swrl:argument1 rdf:resource="#req"/>
                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cost</swrl:argument2>
              </swrl:DatavaluedPropertyAtom>
            </rdf:first>
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
          </swrl:AtomList>
        </rdf:rest>
      </swrl:AtomList>
    </swrl:body>
    <swrl:head>
      <swrl:AtomList>
        <rdf:first>
          <swrl:ClassAtom>
            <swrl:classPredicate rdf:resource="#TemporalAnalysisOpportunity"/>
            <swrl:argument1 rdf:resource="#temporalOpp"/>
          </swrl:ClassAtom>
        </rdf:first>
        <rdf:rest>
          <swrl:AtomList>
            <rdf:first>
              <swrl:IndividualPropertyAtom>
                <swrl:propertyPredicate rdf:resource="#infersOpportunity"/>
                <swrl:argument1 rdf:resource="#req"/>
                <swrl:argument2 rdf:resource="#temporalOpp"/>
              </swrl:IndividualPropertyAtom>
            </rdf:first>
            <rdf:rest>
              <swrl:AtomList>
                <rdf:first>
                  <swrl:DatavaluedPropertyAtom>
                    <swrl:propertyPredicate rdf:resource="#confidenceLevel"/>
                    <swrl:argument1 rdf:resource="#temporalOpp"/>
                    <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.90</swrl:argument2>
                  </swrl:DatavaluedPropertyAtom>
                </rdf:first>
                <rdf:rest>
                  <swrl:AtomList>
                    <rdf:first>
                      <swrl:DatavaluedPropertyAtom>
                        <swrl:propertyPredicate rdf:resource="#reasoningTrace"/>
                        <swrl:argument1 rdf:resource="#temporalOpp"/>
                        <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cost tracking implies temporal trend analysis and spatial cost variations</swrl:argument2>
                      </swrl:DatavaluedPropertyAtom>
                    </rdf:first>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </swrl:AtomList>
                </rdf:rest>
              </swrl:AtomList>
            </rdf:rest>
          </swrl:AtomList>
        </rdf:rest>
      </swrl:AtomList>
    </swrl:head>
  </swrl:Imp>

